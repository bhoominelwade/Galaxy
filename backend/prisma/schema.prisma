datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Transaction {
  id        Int      @id @default(autoincrement())
  hash      String   @unique
  timestamp DateTime
  amount    Float
  createdAt DateTime @default(now())

  @@index([timestamp])
}